{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "deserve-trade-api",
  "compatibility_date": "2025-08-03",
  "main": "./src/index.tsx",
  "dev": {
    "port": 8787
  },
  "routes": [
    {
      "pattern": "api.deserve.trade",
      "custom_domain": true
    }
  ],
  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  }
}
